{
  "name": "@dangao/node-numerator-cluster",
  "version": "0.12.0",
  "description": "numerator-cluster for nodejs",
  "main": "dist/index.js",
  "repository": "https://github.com/dangaogit/node-numerator-cluster.git",
  "author": "dangaogit",
  "license": "MIT",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "rm -rf ./dist/* && tsc -p ./ -d --listFiles --traceResolution --extendedDiagnostics",
    "test": "jest"
  },
  "keywords": [
    "cluster",
    "DCS",
    "distributed"
  ],
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/node": "^14.11.8",
    "jest": "^26.6.0",
    "ts-jest": "^26.4.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "@dangao/date-util-es": "^0.1.0",
    "@dangao/node-log": "^1.2.0"
  },
  "jest": {
    "testEnvironment": "node",
    "testRegex": "./tests/.*\\.jest.(js|ts|tsx)$",
    "coveragePathIgnorePatterns": [
      "<rootDir>/.*/__test__/.*",
      "node_modules",
      "coverage",
      "tests"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "./tsconfig.json",
        "isolatedModules": true
      }
    }
  }
}
